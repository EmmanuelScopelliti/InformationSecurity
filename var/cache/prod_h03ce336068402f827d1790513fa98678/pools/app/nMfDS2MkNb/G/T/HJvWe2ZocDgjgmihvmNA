ato...";}s:12:"imagesFilter";a:4:{s:5:"label";s:8:"Immagini";s:11:"placeholder";s:36:"Seleziona uno stato dell'immagine...";s:15:"textHasCriteria";s:41:"Almeno un'immagine del prodotto assegnata";s:14:"textNoCriteria";s:39:"Nessuna immagine del prodotto assegnata";}s:17:"releaseDateFilter";a:1:{s:5:"label";s:16:"Data di rilascio";}s:11:"stockFilter";a:1:{s:5:"label";s:5:"Stock";}s:11:"priceFilter";a:1:{s:5:"label";s:6:"Prezzo";}s:18:"manufacturerFilter";a:2:{s:5:"label";s:10:"Produttore";s:11:"placeholder";s:23:"Seleziona produttore...";}s:19:"salesChannelsFilter";a:2:{s:5:"label";s:17:"Canali di vendita";s:11:"placeholder";s:30:"Seleziona canali di vendita...";}s:16:"categoriesFilter";a:2:{s:5:"label";s:9:"Categorie";s:11:"placeholder";s:22:"Seleziona categorie...";}s:10:"tagsFilter";a:2:{s:5:"label";s:3:"Tag";s:11:"placeholder";s:16:"Seleziona tag...";}s:17:"messageEmptyTitle";s:13:"Nulla trovato";s:19:"messageEmptySubline";s:43:"Modificare i filtri per vedere i risultati.";}s:12:"notification";a:1:{s:43:"notificationSaveErrorProductNoAlreadyExists";s:50:"N. prodotto "{productNo}" è già stato assegnato.";}}s:17:"sw-product-stream";a:6:{s:7:"general";a:4:{s:19:"mainMenuItemGeneral";s:31:"Categorie di prodotti dinamiche";s:20:"placeholderSearchBar";s:51:"Sfoglia tutte le categorie di prodotto dinamiche...";s:21:"descriptionTextModule";s:52:"Gestisci qui le tue categorie di prodotti dinamiche.";s:13:"errorCategory";s:181:""{name}" fornisce incarichi del prodotto per una categoria e non è dunque eliminabile. | "{name}" fornisce incarichi del prodotto per {count} categorie e non è dunque eliminabile.";}s:3:"acl";a:2:{s:22:"conditionsAlertHeading";s:27:"Non hai abbastanza permessi";s:26:"conditionsAlertDescription";s:84:"Non hai abbastanza permessi per eseguire tutte le azioni nella sezione "Condizioni".";}s:4:"list";a:9:{s:25:"textProductStreamOverview";s:31:"Categorie di prodotti dinamiche";s:10:"columnName";s:4:"Nome";s:17:"columnDescription";s:11:"Descrizione";s:17: