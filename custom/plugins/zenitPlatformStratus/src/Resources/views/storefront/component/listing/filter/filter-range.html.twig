{% sw_extends '@Storefront/storefront/component/listing/filter/filter-range.html.twig' %}

{% block component_filter_range_toggle_icon %}
    {% if theme_config('zen-category-filter-icon') is same as ('plus') %}
        {% sw_icon 'plus' style {
            'pack': 'solid', 'size': 'xs', 'class': 'filter-panel-item-toggle'
        } %}
    {% else %}
        {{ parent() }}
    {% endif %}
{% endblock %}
